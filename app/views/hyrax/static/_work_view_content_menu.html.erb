<!-- @presenter.menu = <%= @presenter.menu %> -->
<!-- @presenter.menu_file_format = <%= @presenter.menu_file_format %> -->
<!-- @presenter.menu_partial = <%= @presenter.menu_partial %> -->
<!-- @presenter.page_navigation = <%= @presenter.page_navigation %> -->
<% if @presenter.menu_partial.present? %>
  <%= render partial: @presenter.menu_partial %>
<% elsif @presenter.menu_links.present? %>
  <% case @presenter.menu_file_format %>
  <% when '.txt' %>
    <%= render partial: 'hyrax/static/work_view_content_menu_from_txt' %>
  <% when '.yml' %>
    <%= render partial: 'hyrax/static/work_view_content_menu_from_yml' %>
  <% when '.yaml' %>
    <%= render partial: 'hyrax/static/work_view_content_menu_from_yml' %>
  <% else %>
    Unknown Menu File Format: <%= @presenter.menu_file_format %>
  <% end %>
<% end %>
