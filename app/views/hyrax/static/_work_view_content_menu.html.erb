<!-- @presenter.static_content_menu = <%= @presenter.static_content_menu %> -->
<!-- @presenter.static_content_menu_file_format = <%= @presenter.static_content_menu_file_format %> -->
<!-- @presenter.static_content_menu_partial = <%= @presenter.static_content_menu_partial %> -->
<!-- @presenter.static_content_page_navigation = <%= @presenter.static_content_page_navigation %> -->
<% if @presenter.static_content_menu_partial.present? %>
  <%= render partial: @presenter.static_content_menu_partial %>
<% elsif @presenter.static_content_menu_links.present? %>
  <% case @presenter.static_content_menu_file_format %>
  <% when '.txt' %>
    <%= render partial: 'hyrax/static/work_view_content_menu_from_txt' %>
  <% when '.yml' %>
    <%= render partial: 'hyrax/static/work_view_content_menu_from_yml' %>
  <% when '.yaml' %>
    <%= render partial: 'hyrax/static/work_view_content_menu_from_yml' %>
  <% else %>
    Unknown Menu File Format: <%= @presenter.static_content_menu_file_format %>
  <% end %>
<% end %>
